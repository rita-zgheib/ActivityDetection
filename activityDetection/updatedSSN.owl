############################
#   Object Properties
############################

# Object Property: ssn:attachedSystem (attached system)

AnnotationAssertion(rdfs:comment ssn:attachedSystem "Relation between a Platform and any Systems (e.g., Sensors) that are attached to the Platform.")
AnnotationAssertion(rdfs:isDefinedBy ssn:attachedSystem "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:attachedSystem "attached system")
AnnotationAssertion(rdfs:seeAlso ssn:attachedSystem "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#PlatformSite")
SubObjectPropertyOf(ssn:attachedSystem DUL:isLocationOf)
InverseObjectProperties(ssn:attachedSystem ssn:onPlatform)

# Object Property: ssn:deployedOnPlatform (deployed on platform)

AnnotationAssertion(rdfs:comment ssn:deployedOnPlatform "Relation between a deployment and the platform on which the system was deployed.")
AnnotationAssertion(rdfs:isDefinedBy ssn:deployedOnPlatform "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:deployedOnPlatform "deployed on platform")
AnnotationAssertion(rdfs:seeAlso ssn:deployedOnPlatform "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#Deployment")
SubObjectPropertyOf(ssn:deployedOnPlatform DUL:hasParticipant)
InverseObjectProperties(ssn:deployedOnPlatform ssn:inDeployment)

# Object Property: ssn:deployedSystem (deployed system)

AnnotationAssertion(rdfs:comment ssn:deployedSystem "Relation between a deployment and the deployed system.")
AnnotationAssertion(rdfs:isDefinedBy ssn:deployedSystem "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:deployedSystem "deployed system")
AnnotationAssertion(rdfs:seeAlso ssn:deployedSystem "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#Deployment")
SubObjectPropertyOf(ssn:deployedSystem DUL:hasParticipant)
InverseObjectProperties(ssn:deployedSystem ssn:hasDeployment)

# Object Property: ssn:deploymentProcessPart (deployment process part)

AnnotationAssertion(rdfs:comment ssn:deploymentProcessPart "Has part relation between a deployment process and its constituent processes.")
AnnotationAssertion(rdfs:isDefinedBy ssn:deploymentProcessPart "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:deploymentProcessPart "deployment process part")
AnnotationAssertion(rdfs:seeAlso ssn:deploymentProcessPart "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#Deployment")
SubObjectPropertyOf(ssn:deploymentProcessPart DUL:hasPart)

# Object Property: ssn:detects (detects)

AnnotationAssertion(rdfs:comment ssn:detects "A relation from a sensor to the Stimulus that the sensor can detect.   
The Stimulus itself will be serving as a proxy for (see isProxyOf) some observable property.")
AnnotationAssertion(rdfs:isDefinedBy ssn:detects "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:detects "detects")
AnnotationAssertion(rdfs:seeAlso ssn:detects "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")

# Object Property: ssn:endTime (end time)

AnnotationAssertion(rdfs:isDefinedBy ssn:endTime "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:endTime "end time")
AnnotationAssertion(rdfs:seeAlso ssn:endTime "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Base#Time")
SubObjectPropertyOf(ssn:endTime DUL:hasRegion)

# Object Property: ssn:featureOfInterest (feature of interest)

AnnotationAssertion(dc:source ssn:featureOfInterest "skos:exactMatch 'featureOfInterest' [O&M - ISO/DIS 19156] 
		                    http://portal.opengeospatial.org/files/?artifact_id=41579")
AnnotationAssertion(rdfs:comment ssn:featureOfInterest "A relation between an observation and the entity whose quality was observed.   For example, in an observation of the weight of a person, the feature of interest is the person and the quality is weight.")
AnnotationAssertion(rdfs:isDefinedBy ssn:featureOfInterest "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:featureOfInterest "feature of interest")
AnnotationAssertion(rdfs:seeAlso ssn:featureOfInterest "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")
SubObjectPropertyOf(ssn:featureOfInterest DUL:isSettingFor)

# Object Property: ssn:forProperty (for property)

AnnotationAssertion(rdfs:comment ssn:forProperty "A relation between some aspect of a sensing entity and a property.  For example, from a sensor to the properties it can observe, or from a deployment to the properties it was installed to observe.  Also from a measurement capability to the property the capability is described for.  (Used in conjunction with ofFeature).")
AnnotationAssertion(rdfs:isDefinedBy ssn:forProperty "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:forProperty "for property")
AnnotationAssertion(rdfs:seeAlso ssn:forProperty "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")

# Object Property: ssn:hasDeployment (has deployment)

AnnotationAssertion(rdfs:comment ssn:hasDeployment "Relation between a System and a Deployment, recording that the System/Sensor was deployed in that Deployment.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasDeployment "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasDeployment "has deployment")
AnnotationAssertion(rdfs:seeAlso ssn:hasDeployment "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#Deployment")
SubObjectPropertyOf(ssn:hasDeployment DUL:isParticipantIn)

# Object Property: ssn:hasInput (has input)

AnnotationAssertion(rdfs:isDefinedBy ssn:hasInput "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasInput "has input")
AnnotationAssertion(rdfs:seeAlso ssn:hasInput "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Model#Process")

# Object Property: ssn:hasMeasurementCapability (has measurement  capability)

AnnotationAssertion(rdfs:comment ssn:hasMeasurementCapability "Relation from a Sensor to a MeasurementCapability describing the measurement properties of the sensor.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasMeasurementCapability "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasMeasurementCapability "has measurement  capability")
AnnotationAssertion(rdfs:seeAlso ssn:hasMeasurementCapability "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability")
SubObjectPropertyOf(ssn:hasMeasurementCapability ssn:hasProperty)

# Object Property: ssn:hasMeasurementProperty (has measurement property)

AnnotationAssertion(rdfs:comment ssn:hasMeasurementProperty "Relation from a MeasurementCapability to a MeasurementProperty.  For example, to an accuracy (see notes at MeasurementCapability).")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasMeasurementProperty "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasMeasurementProperty "has measurement property")
AnnotationAssertion(rdfs:seeAlso ssn:hasMeasurementProperty "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability")
SubObjectPropertyOf(ssn:hasMeasurementProperty ssn:hasProperty)

# Object Property: ssn:hasOperatingProperty (has operating property)

AnnotationAssertion(rdfs:comment ssn:hasOperatingProperty "Relation from an OperatingRange to a Property.  For example, to a battery lifetime.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasOperatingProperty "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasOperatingProperty "has operating property")
AnnotationAssertion(rdfs:seeAlso ssn:hasOperatingProperty "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction")
SubObjectPropertyOf(ssn:hasOperatingProperty ssn:hasProperty)

# Object Property: ssn:hasOperatingRange (has operating range)

AnnotationAssertion(rdfs:comment ssn:hasOperatingRange "Relation from a System to an OperatingRange describing the normal operating environment of the System.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasOperatingRange "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasOperatingRange "has operating range")
AnnotationAssertion(rdfs:seeAlso ssn:hasOperatingRange "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction")
SubObjectPropertyOf(ssn:hasOperatingRange ssn:hasProperty)

# Object Property: ssn:hasOutput (has output)

AnnotationAssertion(rdfs:isDefinedBy ssn:hasOutput "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasOutput "has output")
AnnotationAssertion(rdfs:seeAlso ssn:hasOutput "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Model#Process")

# Object Property: ssn:hasProperty (has property)

AnnotationAssertion(rdfs:comment ssn:hasProperty "A relation between a FeatureOfInterest and a Property of that feature.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasProperty "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasProperty "has property")
AnnotationAssertion(rdfs:seeAlso ssn:hasProperty "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")
SubObjectPropertyOf(ssn:hasProperty DUL:hasQuality)
InverseObjectProperties(ssn:hasProperty ssn:isPropertyOf)

# Object Property: ssn:hasSubSystem (has subsystem)

AnnotationAssertion(rdfs:comment ssn:hasSubSystem "Haspart relation between a system and its parts.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasSubSystem "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasSubSystem "has subsystem")
AnnotationAssertion(rdfs:seeAlso ssn:hasSubSystem "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Model#System")
SubObjectPropertyOf(ssn:hasSubSystem DUL:hasPart)

# Object Property: ssn:hasSurvivalProperty (has survival property)

AnnotationAssertion(rdfs:comment ssn:hasSurvivalProperty "Relation from a SurvivalRange to a Property describing the survial range of a system.  For example, to the temperature extreme that a system can withstand before being considered damaged.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasSurvivalProperty "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasSurvivalProperty "has survival property")
AnnotationAssertion(rdfs:seeAlso ssn:hasSurvivalProperty "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction")
SubObjectPropertyOf(ssn:hasSurvivalProperty ssn:hasProperty)

# Object Property: ssn:hasSurvivalRange (has survival range)

AnnotationAssertion(rdfs:comment ssn:hasSurvivalRange "A Relation from a System to a SurvivalRange.")
AnnotationAssertion(rdfs:isDefinedBy ssn:hasSurvivalRange "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasSurvivalRange "has survival range")
AnnotationAssertion(rdfs:seeAlso ssn:hasSurvivalRange "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction")
SubObjectPropertyOf(ssn:hasSurvivalRange ssn:hasProperty)

# Object Property: ssn:hasValue (has value)

AnnotationAssertion(rdfs:isDefinedBy ssn:hasValue "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:hasValue "has value")
AnnotationAssertion(rdfs:seeAlso ssn:hasValue "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Base#Data")
SubObjectPropertyOf(ssn:hasValue DUL:hasRegion)

# Object Property: ssn:implementedBy (implemented by)

AnnotationAssertion(rdfs:comment ssn:implementedBy "A relation between the description of an algorithm, procedure or method and an entity that implements that method in some executable way.  For example, between a scientific measuring method and a sensor the senses via that method.")
AnnotationAssertion(rdfs:isDefinedBy ssn:implementedBy "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:implementedBy "implemented by")
AnnotationAssertion(rdfs:seeAlso ssn:implementedBy "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")
SubObjectPropertyOf(ssn:implementedBy DUL:describes)
InverseObjectProperties(ssn:implementedBy ssn:implements)

# Object Property: ssn:implements (implements)

AnnotationAssertion(rdfs:comment ssn:implements "A relation between an entity that implements a method in some executable way and the description of an algorithm, procedure or method.  For example, between a Sensor and the scientific measuring method that the Sensor uses to observe a Property.")
AnnotationAssertion(rdfs:isDefinedBy ssn:implements "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:implements "implements")
AnnotationAssertion(rdfs:seeAlso ssn:implements "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")
SubObjectPropertyOf(ssn:implements DUL:isDescribedBy)

# Object Property: ssn:inCondition (in condition)

AnnotationAssertion(rdfs:comment ssn:inCondition "Describes the prevailing environmental conditions for MeasurementCapabilites, OperatingConditions and SurvivalRanges.  Used for example to say that a sensor has a particular accuracy in particular conditions.  (see also MeasurementCapability)")
AnnotationAssertion(rdfs:isDefinedBy ssn:inCondition "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:inCondition "in condition")
AnnotationAssertion(rdfs:seeAlso ssn:inCondition "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Base#ConstraintBlock")

# Object Property: ssn:inDeployment (in deployment)

AnnotationAssertion(rdfs:comment ssn:inDeployment "Relation between a Platform and a Deployment, recording that the object was used as a platform for a system/sensor for a particular deployment: as in this PhysicalObject is acting as a Platform inDeployment Deployment.")
AnnotationAssertion(rdfs:isDefinedBy ssn:inDeployment "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:inDeployment "in deployment")
AnnotationAssertion(rdfs:seeAlso ssn:inDeployment "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#Deployment")
SubObjectPropertyOf(ssn:inDeployment DUL:isParticipantIn)

# Object Property: ssn:isProducedBy (is produced by)

AnnotationAssertion(rdfs:comment ssn:isProducedBy "Relation between a producer and a produced entity: for example, between a sensor and the produced output.")
AnnotationAssertion(rdfs:isDefinedBy ssn:isProducedBy "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:isProducedBy "is produced by")
AnnotationAssertion(rdfs:seeAlso ssn:isProducedBy "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Model#Process")

# Object Property: ssn:isPropertyOf (is property of)

AnnotationAssertion(rdfs:comment ssn:isPropertyOf "Relation between a FeatureOfInterest and a Property (a Quality observable by a sensor) of that feature.")
AnnotationAssertion(rdfs:isDefinedBy ssn:isPropertyOf "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:isPropertyOf "is property of")
AnnotationAssertion(rdfs:seeAlso ssn:isPropertyOf "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")
SubObjectPropertyOf(ssn:isPropertyOf DUL:isQualityOf)

# Object Property: ssn:isProxyFor (isProxyFor)

AnnotationAssertion(rdfs:comment ssn:isProxyFor "A relation from a Stimulus to the Property that the Stimulus is serving as a proxy for.  For example, the expansion of the quicksilver is a stimulus that serves as a proxy for temperature, or an increase or decrease in the spinning of cups on a wind sensor is serving as a proxy for wind speed.")
AnnotationAssertion(rdfs:isDefinedBy ssn:isProxyFor "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:isProxyFor "isProxyFor")
AnnotationAssertion(rdfs:seeAlso ssn:isProxyFor "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")

# Object Property: ssn:madeObservation (made observation)

AnnotationAssertion(rdfs:comment ssn:madeObservation "Relation between a Sensor and Observations it has made.")
AnnotationAssertion(rdfs:isDefinedBy ssn:madeObservation "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:madeObservation "made observation")
AnnotationAssertion(rdfs:seeAlso ssn:madeObservation "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Observation#Observation")
SubObjectPropertyOf(ssn:madeObservation DUL:isObjectIncludedIn)
InverseObjectProperties(ssn:madeObservation ssn:observedBy)

# Object Property: ssn:observationResult (observation result)

AnnotationAssertion(dc:source ssn:observationResult "skos:closeMatch 'result' [O&M - ISO/DIS 19156] 
		                    http://portal.opengeospatial.org/files/?artifact_id=41579")
AnnotationAssertion(rdfs:comment ssn:observationResult "Relation linking an Observation (i.e., a description of the context, the Situation, in which the observatioin was made) and a Result, which contains a value representing the value associated with the observed Property.")
AnnotationAssertion(rdfs:isDefinedBy ssn:observationResult "http://purl.oclc.org/NET/ssnx/ssn")
AnnotationAssertion(rdfs:label ssn:observationResult "observation result")
AnnotationAssertion(rdfs:seeAlso ssn:observationResult "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton")
SubObjectPropertyOf(ssn:observationResult DUL:isSettingFor)

# Object Property: ssn:observationResultTime (observation result time)

AnnotationAssertion(dc:source ssn:observationResultTime "http://www.opengeospatial.org/standards/om")
AnnotationAssertion(rdfs:comment ssn:observationResultTime "The result time is the time when the procedure associated with the observation act was applied.")
